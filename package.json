{
  "name": "wdio-typescript",
  "version": "1.0.0",
  "description": "Webdriver.io using typescript",
  "main": "index.js",
  "author": "jensbodal@gmail.com",
  "license": "MIT",
  "scripts": {
    "postinstall": "yarn install:angular",
    "install:angular": "yarn --cwd frontends/angular",
    "start": "wdio",
    "prestart:angular": "yarn install:angular",
    "start:angular": "yarn --cwd frontends/angular ng serve",
    "format": "prettier --write \"**/*.{ts,js,md,json}\"",
    "format:check": "prettier --check \"**/*.{ts,js,md,json}\"",
    "lint": "tslint --config ./tslint.json --project ./tsconfig.json --format verbose",
    "lint:fix": "tslint --config ./tslint.json --project ./tsconfig.json --format verbose --fix",
    "safari-test-case": "wdio -e chrome,safari,firefox"
  },
  "devDependencies": {
    "@types/chai": "4.1.7",
    "@types/mocha": "5.2.6",
    "@types/node": "12.0.2",
    "@wdio/cli": "5.9.1",
    "@wdio/dot-reporter": "^5.7.8",
    "@wdio/local-runner": "^5.9.1",
    "@wdio/mocha-framework": "^5.9.1",
    "@wdio/selenium-standalone-service": "^5.8.4",
    "@wdio/spec-reporter": "^5.8.0",
    "chai": "4.2.0",
    "prettier": "1.17.1",
    "ts-node": "8.1.0",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.16.0",
    "tslint-config-airbnb": "5.11.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-no-unused-expression-chai": "0.1.4",
    "tslint-plugin-prettier": "2.0.1",
    "typescript": "3.4.5",
    "webdriver": "5.9.1",
    "webdriverio": "5.9.1",
    "yargs": "13.2.4"
  }
}
